programa {
	funcao inicio() {
		/*
		3) Faça um Programa que peça dois números e imprima o maior deles. (1pt)
		*/
         
         real n1, n2
         
         escreva("Digite o primeiro número: ")
         leia(n1)
         
         escreva("Digite o segundo número: ")
         leia(n2)
         
         se(n1>n2){
         escreva("O maior número é o: ",n1)
         }senao{
         escreva("O maior número é o: ",n2)
         }

	}
}
